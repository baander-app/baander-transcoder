    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^8.2.4",
    "@types/method-override": "0.0.35",
    "@types/mime-types": "^2.1.4",
