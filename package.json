    "@tsed/di": "7.72.0",
    "@tsed/engines": "7.72.0",
    "@tsed/exceptions": "7.72.0",
    "@tsed/json-mapper": "7.72.0",
    "@tsed/logger": "6.7.5",
    "@tsed/logger-file": "^6.7.5",
    "@tsed/openspec": "7.72.0",
    "@tsed/platform-cache": "7.72.0",
    "@tsed/platform-exceptions": "7.72.0",
    "@tsed/platform-express": "7.72.0",
    "@tsed/platform-log-middleware": "7.72.0",
    "@tsed/platform-middlewares": "7.72.0",
    "@tsed/platform-params": "7.72.0",
    "@tsed/platform-response-filter": "7.72.0",
    "@tsed/platform-views": "7.72.0",
    "@tsed/schema": "7.72.0",
    "@tsed/swagger": "7.72.0",
    "ajv": "^8.13.0",
    "ajv-errors": "^3.0.0",
    "async-mutex": "^0.5.0",
    "barrelsby": "^2.8.1",
    "body-parser": "1.20.2",
