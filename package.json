{
  "name": "bt2",
  "version": "1.0.0",
  "description": "",
  "license": "Apache-2.0",
  "scripts": {
    "build": "bun run barrels && tsc --project tsconfig.compile.json",
    "compile": "bun build --target=bun ./src/index.ts --outfile=./dist/index.js",
    "compile:bin": "bun build --compile ./src/index.ts --outfile=bun-app",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "bun run barrels && node-dev src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "bun run test:lint && bun run test:coverage ",
    "test:unit": "cross-env NODE_ENV=test vitest run",
    "test:watch": "cross-env NODE_ENV=test vitest",
    "test:coverage": "cross-env NODE_ENV=test vitest run --coverage"
  },
  "dependencies": {
    "@tsed/ajv": "7.72.0",
